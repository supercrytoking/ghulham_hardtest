{
  "name": "synfo",
  "version": "1.0.1",
  "description": "A program with which you can retrieve several types of system info (about hardware, software etc.).",
  "main": "dist/lib.js",
  "scripts": {
    "list": "node bin/list-infos.js",
    "docs": "typedoc --out dist_docs/ --exclude \"node_modules/*\" --name \"Synfo documentation\" --readme README.md .",
    "build": "tsc",
    "all": "npm run build && npm run docs"
  },
  "bin": {
    "synfo": "bin/synfo"
  },
  "keywords": ["CLI"],
  "author": "Romein van Buren <romein@vburen.eu>",
  "homepage": "https://projects.romeinvanburen.nl/synfo/",
  "repository": {
    "type": "git",
    "url": "https://github.com/garraflavatra/synfo.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.4.6",
    "rollup": "^2.50.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  }
}
